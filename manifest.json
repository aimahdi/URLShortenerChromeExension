{
    "manifest_version": 2,
    "name": "URL Shortener by AIM",
    "version": "1.0",
    "description": "URL Shortener - Shorten the URLs using CleanURI API",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icon.jpg",
        "default_popup": "urlshortener.html",
        "default_title": "Click to open popup"
    }
}